    "@types/node": "^10.0.3",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/react-helmet": "^5.0.6",
    "bootstrap": "^4.1.0",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "gatsby": "^1.9.256",
    "gatsby-link": "^1.6.41",
    "gatsby-plugin-catch-links": "^1.0.18",
    "gatsby-plugin-manifest": "^1.0.21",
    "gatsby-plugin-nprogress": "^1.0.13",
    "gatsby-plugin-offline": "^1.0.15",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-sass": "^1.0.26",
    "gatsby-plugin-sharp": "^1.6.44",
    "gatsby-plugin-sitemap": "^1.2.23",
    "gatsby-plugin-twitter": "^1.0.18",
    "gatsby-plugin-typescript": "^1.4.20",
    "gatsby-remark-autolink-headers": "^1.4.17",
    "gatsby-remark-copy-linked-files": "^1.5.32",
    "gatsby-remark-images": "^1.5.63",
    "gatsby-remark-prismjs": "^2.0.2",
    "gatsby-remark-responsive-iframe": "^1.4.18",
    "gatsby-source-filesystem": "^1.5.35",
    "gatsby-transformer-json": "^1.0.17",
    "gatsby-transformer-remark": "^1.7.40",
    "gatsby-transformer-sharp": "^1.6.24",
    "lodash": "^4.17.10",
    "lodash-webpack-plugin": "^0.11.5",
    "prismjs": "^1.14.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --parser typescript --write \"src/**/*.ts*\""
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-typescript": "^1.0.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-typescript": "^0.12.0",
    "prettier": "^1.12.1",
    "typescript": "^2.8.3",
    "typescript-eslint-parser": "^15.0.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:jsx-a11y/recommended",
      "typescript",
      "typescript/react",
      "typescript/prettier"
    ],
    "plugins": [
      "jsx-a11y"
    ],
    "rules": {
      "semi": "error",
      "no-undef": "off",
      "no-unused-vars": "off",
      "no-dupe-class-members": "off",
      "require-jsdoc": "error",
      "valid-jsdoc": [
        "error",
        {
          "preferType": {
            "any": "*",
            "object": "Object"
          },
          "requireReturnType": true,
          "requireParamDescription": true,
          "requireReturnDescription": true
        }
      ],
      "quotes": [
        "error",
        "double",
        {
          "avoidEscape": true
        }
      ],
      "no-console": "off"
    }
  }
}
