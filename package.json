  "name": "weirdpattern.com",
  "description": "Isn't it weird?",
  "author": "Patricio Trevino <patricio@weirdpattern.com>",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "elasticlunr": "^0.9.5",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.15",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.0.0-beta"
  },
  "devDependencies": {
    "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "^2.3.0",
    "@types/classnames": "^2.2.9",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "copyfiles": "^2.1.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "gatsby": "^2.13.32",
    "gatsby-image": "^2.2.6",
    "gatsby-plugin-catch-links": "^2.1.2",
    "gatsby-plugin-lodash": "^3.1.2",
    "gatsby-plugin-manifest": "^2.2.3",
    "gatsby-plugin-netlify": "^2.1.3",
    "gatsby-plugin-nprogress": "^2.1.2",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-sass": "^2.1.3",
    "gatsby-plugin-sharp": "^2.2.8",
    "gatsby-plugin-sitemap": "^2.2.3",
    "gatsby-plugin-twitter": "^2.1.2",
    "gatsby-plugin-typescript": "^2.1.2",
    "gatsby-remark-copy-linked-files": "^2.1.3",
    "gatsby-remark-embed-gist": "^1.1.9",
    "gatsby-remark-images": "^3.1.7",
    "gatsby-remark-responsive-iframe": "^2.2.4",
    "gatsby-source-filesystem": "^2.1.5",
    "gatsby-transformer-json": "^2.2.2",
    "gatsby-transformer-remark": "^2.6.8",
    "gatsby-transformer-sharp": "^2.2.4",
    "lodash-webpack-plugin": "^0.11.5",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  },
  "resolutions": {
    "graphql": "14.1.1"
  },
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "lint": "eslint --ext .ts,.tsx src/",
    "format": "prettier --parser typescript --write \"src/**/*.ts*\""
  },
  "eslintConfig": {
    "extends": [
      "plugin:jsx-a11y/recommended",
      "typescript",
      "typescript/react",
      "typescript/prettier"
    ],
    "plugins": [
      "jsx-a11y"
    ],
    "rules": {
      "semi": "error",
      "no-undef": "off",
      "no-unused-vars": "off",
      "no-dupe-class-members": "off",
      "require-jsdoc": "error",
      "valid-jsdoc": [
        "error",
        {
          "preferType": {
            "any": "*",
            "object": "Object"
          },
          "requireReturnType": true,
          "requireParamDescription": true,
          "requireReturnDescription": true
        }
      ],
      "quotes": [
        "error",
        "double",
        {
          "avoidEscape": true
        }
      ],
      "no-console": "off"
    }
  }
}
